{
  "name": "ai-starter",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*",
    "tests"
  ],
  "scripts": {
    "dev": "bun --filter './apps/*' dev",
    "db:studio": "bunx drizzle-kit studio",
    "db:push": "bunx drizzle-kit push",
    "cli": "bun apps/cli/index.ts",
    "api": "bun apps/api/index.ts",
    "web": "bun --watch apps/api/index.ts",
    "worker": "bun apps/worker/index.ts",
    "worker:watch": "bun apps/worker/index.ts --watch",
    "test": "bun test",
    "eval:dev": "evalite watch",
    "eval": "evalite",
    "typecheck": "tsc --build --noEmit --force",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "bun format && eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "bun run lint && bun run typecheck && bun run test # && bun run eval --threshold=95"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@opencode-ai/plugin": "^1.0.132",
    "better-sqlite3": "^12.4.6",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.39.1",
    "evalite": "^1.0.0-beta.15",
    "prettier": "^3.2.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  }
}
